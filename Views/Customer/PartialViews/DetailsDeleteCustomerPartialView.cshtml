    <label class="col-form-label" asp-for="Orders.Count"></label>
    <input asp-for="Orders.Count" class="form-control" readonly />
</div>
<div class="form-group">
    <label class="col-form-label" asp-for="Reviews.Count"></label>
    <input asp-for="Reviews.Count" class="form-control" readonly />
</div>
<div class="form-group">
    <label class="col-form-label" asp-for="PaymentMethods.Count"></label>
    <input asp-for="PaymentMethods.Count" class="form-control" readonly />
</div>